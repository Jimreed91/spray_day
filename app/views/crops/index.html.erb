<h1>Crops</h1>
<p></p>
<ul class="list-inline">
  <%= link_to "Add", crops_new_url, class: "btn btn-success" %>
</ul>
<table class="table table-hover table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Crop</th>
      <th scope="col">Year</th>
      <th scope="col">Hectares</th>
      <th scope="col">Location</th>
    </tr>
  </thead>
  <tbody>
     <%= render 'row' %>
   </tbody>
</table>
