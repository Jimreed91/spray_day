 <% @products.each_with_index do |p, i| %>

    <tr>
      <th scope="row"><%= "#{i + 1}" %></th>
      <td><%= p.name %></td>
      <td><%= p.rate_per_ha %></td>
      <td><%= p.liquid %></td>
      <td><%= p.farm.name %></td>
      <td><%= render 'shared/model_dropdown', model: p %></td>
    </tr>

<% end %>
